<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <TextView
        android:id="@+id/f1name"
        android:layout_width="76dp"
        android:layout_height="34dp"
        android:layout_marginBottom="72dp"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp"
        android:layout_marginTop="102dp"
        android:text="姓名"
        app:layout_constraintBottom_toTopOf="@+id/f1ID"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        android:gravity="center"/>

    <TextView
        android:id="@+id/f1ID"
        android:layout_width="78dp"
        android:layout_height="36dp"
        android:layout_marginBottom="80dp"
        android:layout_marginLeft="36dp"
        android:layout_marginStart="36dp"
        android:text="手机号"
        app:layout_constraintBottom_toTopOf="@+id/f1password"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center"/>

    <TextView
        android:id="@+id/f1password"
        android:layout_width="73dp"
        android:layout_height="34dp"
        android:layout_marginBottom="209dp"
        android:layout_marginLeft="38dp"
        android:layout_marginStart="38dp"
        android:text="密码"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:gravity="center"/>

    <EditText
        android:id="@+id/textView5"
        android:layout_width="162dp"
        android:layout_height="45dp"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        android:layout_marginTop="150dp"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/f1editviewid"
        android:layout_width="162dp"
        android:layout_height="45dp"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        android:layout_marginTop="63dp"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView5" />

    <EditText
        android:id="@+id/f1textView9"
        android:layout_width="162dp"
        android:layout_height="45dp"
        android:layout_marginEnd="47dp"
        android:layout_marginRight="47dp"
        android:layout_marginTop="69dp"
        android:gravity="bottom|center"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/f1editviewid" />

    <Button
        android:id="@+id/f1login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="86dp"
        android:layout_marginEnd="158dp"
        android:layout_marginLeft="138dp"
        android:layout_marginRight="158dp"
        android:layout_marginStart="138dp"
        android:text="登陆"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />
</android.support.constraint.ConstraintLayout>
